<div class="my-3 d-flex justify-content-between align-items-center">
    <h1>User panel</h1>
</div>

<table class="table  table-hover table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Fullname</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach((user, index)=> { %>
            <tr>
                <th scope="row">
                    <%= index + 1 %>
                </th>
                <td>
                    <%= `${user.firstname} ${user.lastName}` %>
                </td>
            </tr>
            <% }) %>

    </tbody>
</table>